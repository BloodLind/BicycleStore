@model IEnumerable<BicycleStore.Identity.Models.User>

    <h2>User list</h2>
    <table class="table">
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Roles</a>
                </td>

                <td>
                    <a class="btn btn-sm btn-primary" asp-action="EditUserData" asp-controller="Admin" asp-route-id="@user.Id">Edit Data</a>
                </td>
            </tr>
        }
    </table>
